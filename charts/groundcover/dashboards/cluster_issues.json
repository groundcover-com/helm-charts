          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "with entity_issues as( SELECT issues.perspective_entity_issue_id, last(issues.perspective_entity_id, issues.bucket_time) as perspective_entity_id, last(issues.product_type, issues.bucket_time) as product_type, last(s.return_code, issues.bucket_time) as return_code, last(s.resource_name, issues.bucket_time) as resource_name, max(issues.bucket_time) as last_seen, count(distinct(span_id)) as incidents FROM issues JOIN _ps_trace.span AS s on $__timeFilter(s.start_time) and s.perspective_entity_issue_id = issues.perspective_entity_issue_id WHERE $__timeFilter(issues.bucket_time) AND issues.product_type != 'container_restart' GROUP by issues.perspective_entity_issue_id) SELECT last(ei.product_type, ei.last_seen) AS issueType, last(ei.return_code, ei.last_seen) As issueCode, last(ei.resource_name, ei.last_seen) AS resourceName, last(e.namespace, ei.last_seen) AS namespace, last(e.name, ei.last_seen) AS workloadName, last(ei.incidents, ei.last_seen) AS incidents FROM entity_issues as ei JOIN _ps_trace.entity AS e on e.hash_id = ei.perspective_entity_id where e.namespace = '$namespace' GROUP by ei.perspective_entity_issue_id, ei.last_seen ORDER by ei.last_seen desc limit 20",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "percentile"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "conn_stats_samples",
          "timeColumn": "\"time\"",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
